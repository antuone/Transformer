{
    "manifest_version": 2,

    "name": "Трансформер",
    "description": "Изменяет страницы",
    "version": "1.0",
    "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": [ "*://*/*" ],
            "js": [ "content.js" ]
        }
    ]
}